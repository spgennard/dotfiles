{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ if .Env.OMP_TITLE_PREFIX }} {{ env \"OMP_TITLE_PREFIX\" }} {{else}} {{.Folder}} {{ end}} @ {{ .HostName }}",
  "palettes": {
    "template": "{{ if eq .Env.TERM_PROGRAM \"vscode\" }}vscode-rocket{{ else }}rocket{{ end }}",
    "list": {
      "rocket": {
        "git-foreground": "#98019b",
        "git-background": "#FFFB38",
        "git": "#FFFB38",
        "git-modified": "#FF9248",
        "git-diverged": "#FF4500",
        "git-ahead": "#B388FF",
        "git-behind": "#B388FF",
        "svn-foreground": "#98019b",
        "svn-background": "#FFFB38",
        "red": "#FF0000",
        "green": "#00FF00",
        "blue": "#0000FF",
        "white": "#FFFFFF",
        "black": "#111111",
        "rs-background-1": "#98019b",
        "rs-background-2": "#98019b",
        "rs-background-3": "#98019b",
        "rs-background-4": "#98019b"
      },
      "vscode-rocket": {
        "git-foreground": "#98019b",
        "git-background": "#FFFFFF",
        "git": "#FFFFFF",
        "git-modified": "#FF9248",
        "git-diverged": "#FF4500",
        "git-ahead": "#B388FF",
        "git-behind": "#B388FF",
        "svn-foreground": "#98019b",
        "svn-background": "#FFFFFF",
        "red": "#FF0000",
        "green": "#00FF00",
        "blue": "#0000FF",
        "white": "#FFFFFF",
        "black": "#111111",
        "rs-background-1": "#98019b",
        "rs-background-2": "#98019b",
        "rs-background-3": "#98019b",
        "rs-background-4": "#98019b"
      }
    }
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:git-foreground",
          "background": "p:git-background",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uea68 ÓÇ± {{ .HEAD }}",
          "type": "git",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-modified{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-diverged{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
          ],
          "mapped_branches": {
            "feat/*": "üöÄ ",
            "bug/*": "üêõ "
          }
        }
      ],
      "newline": false,
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "type": "prompt",
      "segments": [
        {
          "type": "svn",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:svn-foreground",
          "background": "p:svn-background",
          "properties": {
            "fetch_status": false,
            "native_fallback": true
          },
          "template": "\ue0a0 ÓÇ± {{.Branch}} {{.Working.String}}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "{{ if .Env.COBDIR }}COBDIR ÓÇ± {{ env \"COBDIR\" }} {{if .Env.__COBOL_VERSION }}\uebfe {{ env \"__COBOL_VERSION\" }}{{ end }}{{ end }}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "{{ if .Env.ACUCOBOL }}ACUCOBOL ÓÇ± {{ env \"ACUCOBOL\" }} {{if .Env.__COBOL_VERSION }}\uebfe {{ env \"__COBOL_VERSION\" }}{{ end }}{{ end }}"
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "type": "prompt",
      "segments": [
        {
          "background": "p:rs-background-3",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        }
      ]
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:rs-background-4",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "properties": {
            "style": "full"
          },
          "style": "diamond",
          "template": "\uf115 ÓÇ± {{ .Path }}",
          "min_width": 80,
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "type": "path"
        }
      ],
      "newline": true,
      "type": "prompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#ffe9aa",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "type": "session",
          "background": "p:rs-background-2",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if .SSHSession }}ÓÆ© {{ end }}{{ .UserName }}{{ if .SSHSession }}@.HostName{{ end }}"
        },
        {
          "background": "p:red",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue20f ",
          "type": "status"
        },
        {
          "foreground": "#007ACC",
          "style": "plain",
          "template": " \u276f ",
          "type": "text"
        }
      ],
      "newline": true,
      "type": "prompt"
    }
  ],
  "version": 2
}