{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ if .Env.OMP_TITLE_PREFIX }} $OMP_TITLE_PREFIX {{else}} {{.Folder}} {{ end}} - {{ if .WSL }} $WSL_DISTRO_NAME {{else}} {{.OS}} {{end}} ($COLUMNS/$LINES)",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#193549",
          "background": "#ffeb3b",
          "style": "",
          "template": "\uf1d3  {{ .HEAD }}",
          "type": "git"
        }
      ],
      "newline": false,
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "type": "prompt",
      "segments": [
        {
          "type": "svn",
          "style": "powerline",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "properties": {
            "fetch_status": false,
            "native_fallback": true
          },
          "template": "\uea68  {{.Branch}} {{.Working.String}}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "{{ if .Env.COBDIR }}COBDIR  $COBDIR {{if .Env.__COBOL_VERSION }}\uebfe $__COBOL_VERSION{{ end }}{{ end }}"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "{{ if .Env.ACUCOBOL }}ACUCOBOL  $ACUCOBOL {{if .Env.__COBOL_VERSION }}\uebfe $__COBOL_VERSION{{ end }}{{ end }}"
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "type": "prompt",
      "segments": [
        {
          "background": "#906cff",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        }
      ]
    },
    {
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffca1a",
          "background": "#0078d4",
          "template": "{{ if .WSL }}\ued04{{end}} {{.Icon}}   ",
          "properties": {
            "display_distro_name": false
          }
        },
        {
          "background": "#ffe9aa",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        },
        {
          "background": "#ffffff",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#91ddff",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": "\ue0bb",
            "style": "full"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#ff8080",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue20f ",
          "type": "status"
        }
      ],
      "newline": true,
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#007ACC",
          "style": "plain",
          "template": "\u276f ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}